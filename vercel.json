{
  "rewrites": [
    {
      "source": "/((?!.*\\.pdf$|.*\\.mp4$).)*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*\\.pdf)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/pdf"
        }
      ]
    }
  ]
}
